.current-status{margin-top:50rpx;font-weight:bolder;font-family:Open Sans,sans-serif;font-size:50rpx;text-align:center}.feedback-title{margin-top:40rpx;margin-left:60rpx;font-weight:bolder;font-family:Open Sans,sans-serif;font-size:35rpx;color:#666}.feedback-image-box{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-flex-wrap:wrap;flex-wrap:wrap;padding:30rpx}.feedback-image-box .feedback-image-item{text-align:center;margin:auto;position:relative;width:70%;height:0;padding-top:70%;-webkit-box-sizing:border-box;box-sizing:border-box}.feedback-image-box .feedback-image-item .close-icon{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;align-items:center;position:absolute;right:0;top:0;width:48rpx;height:48rpx;-webkit-border-radius:50%;border-radius:50%;background-color:#ff5a5f;z-index:2}.feedback-image-box .feedback-image-item .image-box{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;align-items:center;position:absolute;top:15rpx;right:15rpx;bottom:15rpx;left:15rpx;border:5rpx #eee solid;-webkit-border-radius:15rpx;border-radius:15rpx;overflow:hidden}.feedback-image-box .feedback-image-item .image-box image{width:100%;height:100%}